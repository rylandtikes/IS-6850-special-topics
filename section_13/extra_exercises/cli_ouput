(base) λ ~/Documents/utahschoolwork/fall2020_charles_dockerkubernates/section_13/lab_exercises/simplek8s/ master* cat client-deployment.yaml 
apiVersion: apps/v1
kind: Deployment
metadata:
  name: client-deployment
spec:
  replicas: 1
  selector:
    matchLabels:
      component: web
  template:
    metadata:
      labels:
        component: web
    spec:
      containers:
        - name: client
          image: stephengrider/multi-client
          ports:
            - containerPort: 3000
(base) λ ~/Documents/utahschoolwork/fall2020_charles_dockerkubernates/section_13/lab_exercises/simplek8s/ master* cat client-node-port.yaml 
apiVersion: v1
kind: Service
metadata:
  name: client-node-port
spec:
  type: NodePort
  ports:
    - port: 3050
      targetPort: 3000
      nodePort: 31515
  selector:
    component: web
